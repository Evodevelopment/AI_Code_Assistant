
{
    "name": "ai-code-assistant",
    "displayName": "AI Code Assistant",
    "description": "An AI-powered code assistant using OpenAI API",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.44.0"
    },
    "activationEvents": [
        "onCommand:extension.completeCode"
    ],
    "main": "./extension.js",
    "contributes": {
        "commands": [
            {
                "command": "extension.completeCode",
                "title": "Complete Code"
            }
        ]
    }
}
